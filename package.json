{
  "name": "standard-webpack-frame",
  "version": "1.0.0",
  "description": "jj-standard-webpack-frame",
  "sideEffects": false,
  "scripts": {
    "start": "pnpm dev",
    "dev": "webpack server --config ./config/webpack.dev.config.js",
