    "clear": "rimraf node_modules pnpm-lock.yaml .eslintcache docs dist",
    "commit": "git-cz"
  },
  "keywords": [],
  "author": "joe",
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "4.0.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "only-allow": "^1.1.1",
    "rimraf": "5.0.1",
    "turbo": "1.10.3",
    "webpack": "5.88.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "git-cz": "4.9.0"
  }
}
